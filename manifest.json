{
  "manifest_version": 3,
  "name": "Google Search Results Modifier",
  "version": "1.1",
  "description": "Automatically adds &num=100 to Google search URLs to display 100 results per page.",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "*://*.google.com/*"
  ],
  "action": {
    "default_icon": {
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "hello.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
